        </m:msup>
        <m:mfenced>
          <m:mrow>
            <m:mn>1</m:mn>
            <m:mo>+</m:mo>
            <m:mfrac>
              <m:mn>2</m:mn>
              <m:mi>x</m:mi>
            </m:mfrac>
            <m:mo>+</m:mo>
            <m:mfrac>
              <m:mn>3</m:mn>
              <m:mrow>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mn>4</m:mn>
