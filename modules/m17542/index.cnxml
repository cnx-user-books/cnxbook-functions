<example id="example-162a1">
<para id="element-162a1">
<term>Problem : </term> 
Determine limit :
</para>
<para id="element-162b">
<m:math display="block">
  <m:mrow>
    <m:munderover>
      <m:mi>lim</m:mi>
      <m:mrow>
        <m:mi>x</m:mi>
        <m:mo>→</m:mo>
