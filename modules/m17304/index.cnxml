    <m:mi>α</m:mi>
    <m:mo>,</m:mo>
    <m:mi>β</m:mi>
    <m:mo>}</m:mo>
    <m:mspace width="1em"/>
  </m:mrow>
</m:math>
</para>
<para id="element-71">
<m:math display="block">
  <m:mrow>
    <m:mi>D</m:mi>
