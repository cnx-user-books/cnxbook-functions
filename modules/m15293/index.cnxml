          <m:mo>−</m:mo>
          <m:mn>3</m:mn>
          <m:mi>y</m:mi>
        </m:mrow>
      </m:mfenced>
      <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo>
    <m:mn>4</m:mn>
    <m:mfenced>
      <m:mrow>
        <m:mi>y</m:mi>
        <m:mo>−</m:mo>
