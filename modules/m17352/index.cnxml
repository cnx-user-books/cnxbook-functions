      </m:mrow>
      <m:mn>3</m:mn>
    </m:mfrac>
    <m:mo>;</m:mo>
    <m:mi>n</m:mi>
    <m:mo>∈</m:mo>
    <m:mi>Z</m:mi>
  </m:mrow>
</m:math>

</para>
<para id="element-77">
